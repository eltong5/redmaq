// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Producto {
  id        Int      @id @default(autoincrement())
  nombre    String
  categoria String
  precio    Float
  imagen    String?
  creadoEn  DateTime @default(now())
}
